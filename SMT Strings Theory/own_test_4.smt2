(set-logic UFSLIA)
(declare-fun k () String)
(declare-fun l () String)
(declare-fun m () String)
(declare-fun n () String)
(declare-fun o () String)
(assert (= (str.len k) (+ (str.len l) (str.len m))))
(assert (= (str.++ k n) (str.++ l (str.++ m n))))
(assert (not (= k (str.++ l m))))
(check-sat)
(exit)