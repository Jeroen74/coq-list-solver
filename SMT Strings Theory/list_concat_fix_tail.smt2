(set-logic UFSLIA)
(declare-fun la () String)
(declare-fun lb () String)
(declare-fun l1 () String)
(declare-fun l2 () String)
(declare-fun l3 () String)
(declare-fun l3alt () String)
(assert (= (str.++ la (str.++ lb l3)) (str.++ l1 (str.++ l2 l3alt))))
(assert (= (+ (str.len la) (str.len lb)) (+ (str.len l1) (str.len l2))))
(assert (not (= l3 l3alt)))
(check-sat)
(exit)