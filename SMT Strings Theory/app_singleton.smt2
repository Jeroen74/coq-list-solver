(set-logic UFSLIA)
(declare-fun s1 () String)
(declare-fun s2 () String)
(declare-fun sx () String)
(declare-fun A () Bool)
(declare-fun B () Bool)
(assert (= (str.len sx) 1))
(assert (= A (= (str.++ s1 s2) sx) ))
(assert (= B ( or (and (= (str.len s1) 0) (= s2 sx) ) (and (= s1 sx) (= (str.len s2) 0)) )))
(assert (or (and (not A) B ) (and A (not B) )))
(check-sat)
(exit)
