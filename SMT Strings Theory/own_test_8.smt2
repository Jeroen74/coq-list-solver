(set-logic UFSLIA)
(declare-fun la () String)
(declare-fun lb () String)
(declare-fun lc () String)
(declare-fun ld () String)
(declare-fun l1 () String)
(declare-fun l2 () String)
(declare-fun l3 () String)
(declare-fun l12 () String)
(declare-fun l23 () String)
(declare-fun l1alt () String)
(assert (= l12 (str.++ la lb)))
(assert (= l23 (str.++ lc ld)))
(assert (= l12 (str.++ l1alt l2)))
(assert (= l23 (str.++ l2 l3)))
(assert (= (str.++ l12 l3) (str.++ l1 l23)))
(assert (not (= l1 l1alt)))
(check-sat)
(exit)