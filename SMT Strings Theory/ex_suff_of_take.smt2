(set-logic UFSLIA)
(define-fun sublist ((n Int) (m Int) (s String)) String
	(ite (< n 0) (str.substr s 0 m) 
	(ite (< m 0) (str.substr s n 0)
		(str.substr s n (- m n))
	)))
(declare-fun n () Int)
(declare-fun l1 () String)
(declare-fun l2 () String)
(declare-fun x () String)
(assert (= l1 (sublist 0 n l2)))
(assert (not (exists ((x String)) (= (str.++ l1 x) l2))))
(check-sat)
(exit)